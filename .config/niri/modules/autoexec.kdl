// Execute programs during Niri startup
spawn-at-startup "waybar"
spawn-sh-at-startup "playerctld daemon"
spawn-at-startup "mpd"
spawn-at-startup "swaync"
spawn-sh-at-startup "emacs --daemon"
spawn-sh-at-startup "swaybg -i ~/Pictures/Wallpapers/frieren.jpg -m fill"
spawn-sh-at-startup "swayidle -w timeout 601 'niri msg action power-off-monitors' timeout 600 'swaylock -f' before-sleep 'swaylock -f'"
